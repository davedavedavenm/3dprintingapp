# Frontend Environment Configuration Template
# Copy this file to .env and replace with actual values

# PayPal Configuration
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_sandbox_client_id
REACT_APP_PAYPAL_ENVIRONMENT=sandbox

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_VERSION=v1

# App Configuration
REACT_APP_NAME=3D Print Quoting System
REACT_APP_VERSION=1.0.0
REACT_APP_SUPPORT_EMAIL=support@3dprintservice.com
REACT_APP_SUPPORT_PHONE=+1-555-123-4567

# Feature Flags
REACT_APP_ENABLE_3D_PREVIEW=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false

# Upload Configuration
REACT_APP_MAX_FILE_SIZE=104857600
REACT_APP_ALLOWED_FILE_TYPES=.stl,.obj,.ply

# Sentry Error Tracking (Optional)
# REACT_APP_SENTRY_DSN=your_sentry_dsn

# Google Analytics (Optional)
# REACT_APP_GA_TRACKING_ID=your_ga_tracking_id
